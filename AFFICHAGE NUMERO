#include <stdio.h>
#include <stdlib.h>
#define MAX 1000
#include <string.h>
typedef struct donnee
{
    char numero[13];
    char nom[30];
    char prenom[30];
    char entsort[8];
    int booleen;
} don;

FILE* chargerfichier(FILE* fichier)
{
    fichier = NULL;
    fichier = fopen("releve.txt","r");
    if(fichier == NULL)
    {
        printf("erreur d'ouverture de fichier.");
    }
return(fichier);
}



int main()
{
    FILE* RELEVE;
    RELEVE = NULL;
    don numeros;
    RELEVE = chargerfichier(RELEVE);
    fscanf(RELEVE, "%s", &numeros.numero);
    printf("%s\n", numeros.numero);
    return 0;
}
